  "discardResponseBodies": true,
  "scenarios": {
    "api_endpoints_config": {
      "executor": "constant-arrival-rate",
      "rate": 50,
      "duration": "10s",
      "preAllocatedVUs": 5,
      "maxVUs": 100,
      "exec": "test_api_endpoints_config"
